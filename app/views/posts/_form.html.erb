<%= form_for(@post) do |f| %>

  <div class="field">
    <%= f.label :Title %><br>
    <%= f.text_field :title %>
  </div>
  <div class="field">
    <%= f.label :Text%><br>
    <%= f.text_area :body, placeholder: "Compose new post..." %>
 
  </div>
  
  <%= f.submit "Post", class: "btn btn-primary"%>

  
 
  <% end %>

